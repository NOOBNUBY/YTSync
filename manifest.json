{
    "manifest_version": 3,
    "name": "YTSync",
    "version": "1.0.2",
    "description": "유트브 라이브의 싱크를 맞춰주는 확장 프로그램입니다.",

    "icons": {
        "16": "img/ytsync16.png",
        "32": "img/ytsync32.png",
        "48": "img/ytsync48.png",
        "128": "img/ytsync128.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["main.js"]
        }
    ]
}