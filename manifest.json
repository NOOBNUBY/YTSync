{
    "manifest_version": 3,
    "name": "YTSync",
    "version": "1.0.0",
    "description": "An extension that automatically synchronizes live sync for YouTube.",

    "icons": {
        "16": "img/youtube.png",
        "32": "img/youtube.png",
        "48": "img/youtube.png",
        "128": "img/youtube.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["main.js"]
        }
    ]
}